<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teacher</title>
  </head>
  <body>
    <table>


    <tr>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
    <%dataTeachers.forEach (row=> {%>
    <tr>
      <td><%= row.id%></td>
      <td><%=row.first_name%></td>
      <td><%=row.last_name%></td>
      <td><%=row.email%></td>

      </tr>
      <%})%>
    </table>
  </body>
</html>
